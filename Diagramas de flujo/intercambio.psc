Algoritmo Intercambio
	Definir varA Como Real;
	Definir varB Como Real;
	Definir auxVar Como Real;
	Escribir "Ingrese variable A";
	Leer varA;
	Escribir "Ingrese variable B";
	Leer varB;
	Escribir "Variable A: ",varA;
	Escribir "Variable B: ",varB;
	Escribir "Intercambiando...";
	auxVar <- varA;
	varA <- varB;
	varB <- auxVar;
	Escribir "Variable A: ",varA;
	Escribir "Variable B: ",varB;
FinAlgoritmo

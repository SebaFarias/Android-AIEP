Algoritmo Porcentaje
	Definir num Como Real;
	Definir percent Como Real;
	Definir value Como Real;
	Escribir "Ingrese un numero real";
	Leer num;
	Escribir "Ingrese el porcentaje que desea calcular";
	Leer percent;
	value <- num * percent / 100; 
	Escribir "El ",percent, "% de ", num ," es ", value ;
FinAlgoritmo
